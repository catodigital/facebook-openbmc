+		"","","","",
+		"","","presence-cmm","",
+		"","","","leakage-detect-alert",
+		"","","","";
 	};
 
 	i2c-mux@71 {
@@ -370,6 +400,10 @@ &i2c11 {
 
 &i2c12 {
 	status = "okay";
+	retimer@24 {
+		compatible = "asteralabs,pt5161l";
+		reg = <0x24>;
+	};
 };
 
 &i2c13 {
@@ -463,7 +497,8 @@ &gpio0 {
 	/*A0-A7*/	"","","","","","","","",
 	/*B0-B7*/	"","","","",
